                "extension": ".txt"
            },
            "boldref2anat": {
                "datatype": "func",
                "from": "boldref",
                "to": ["anat", "T1w", "T2w"],
                "mode": "image",
                "suffix": "xfm",
                "extension": ".txt"
            },
            "boldref2fmap": {
                "datatype": "func",
                "from": "orig",
                "mode": "image",
                "suffix": "xfm",
                "extension": ".txt"
            },
            "anat2mni152nlin6asym": {
                "datatype": "anat",
