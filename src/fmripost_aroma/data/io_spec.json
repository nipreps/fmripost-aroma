    },
    "entities": [
        {
            "name": "fmapid",
            "pattern": "fmapid-([a-zA-Z0-9]+)"
        },
        {
            "name": "cohort",
            "pattern": "(?:^|_)cohort-([0-9]+)",
            "dtype": "int"
        },
        {
            "name": "segmentation",
            "pattern": "seg-([a-zA-Z0-9]+)"
        },
        {
            "name": "statistic",
            "pattern": "(?:^|_)stat-([a-zA-Z0-9]+)"
        },
        {
            "name": "threshold",
            "pattern": "(?:^|_)thresh-([a-zA-Z0-9]+)"
        }
      ],
